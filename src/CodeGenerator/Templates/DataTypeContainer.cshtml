@using ELifeRPG.CodeGenerator.Templates
@inherits RazorLight.TemplatePage<DataTypeContainer>
@{
    DisableEncoding = true;
}
@Model.DataType.DataTypeName @Model.DataType.TypeName @(Model.DataType.BaseDataType is null ? string.Empty : $": {Model.DataType.BaseDataType}")
{
@if (Model.ClassDataType is not null)
{
    await IncludeAsync("ClassDataTypeBody", new ClassDataTypeBody(Model.ClassDataType));
} 
else if (Model.EnumDataType is not null)
{
    await IncludeAsync("EnumDataTypeBody", new EnumDataTypeBody(Model.EnumDataType));
}
}
